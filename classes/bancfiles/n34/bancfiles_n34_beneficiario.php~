<? defined('SYSPATH') or die('no direct access allowed');

class bancfiles_n34_beneficiario {

      private $data;

      public function __set($name, $value){

            switch ($name){
                case 'importe':
                case 'control':
                case 'nif':
                case 'nombre':
                case 'cuenta':
                    break;
                
                default:
            }
      
            
            $this->data[$name] = $value;          
      
      
      }

      public function __get($name){
      
            if (in_array($name,array('importe','control','nif','nombre','cuenta')) && array_key_exists($name,$this->data){
                return $this->data[$name];
            }
            
            return NULL;
      }

      public function values(array $values){
      
          
      }


}

